<div>
  <% @users.each do |user| %>
    <div>
      <div>
        <img src="<%= user.photo %>" width="100" height="100" alt="Profile picture">
      </div>
      <div>
        <div>
         <h1>
          <%= user.name %>
          </h1>
          <div>
            <p> Number of posts: <%= user.posts_counter %> </p>
         </div>
        </div>
      </div>
    </div>
    <% end %>
</div>